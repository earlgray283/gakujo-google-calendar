{
  // Go
  "go.lintTool": "golangci-lint",
  "go.formatTool": "gofmt",
  "go.testFlags": [
    "-v",
    "-count=1"
  ],
  "[go]": {
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    },
    "editor.suggest.snippetsPreventQuickSuggestions": false
  },
  "cSpell.words": [
    "classenq",
    "gakujo"
  ]
}
